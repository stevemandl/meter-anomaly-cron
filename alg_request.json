{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "Detction algorithm parameter schema",
    "required": ["pointName"],
    "properties": {
      "pointName": {
        "type": "string",
        "title": "Name of point to analyze",
        "default": "",
        "pattern": "^[-a-zA-Z0-9\\$_]+(([.]|/)[-A-Za-z0-9\\$_]+)*$"
      },
      "timeStamp": {
        "type": "number",
        "title": "Absolute time in ms since the epoch. Detection will be as of this time, defaults to current time."
      }
    }
  }